{
    "name": "paranoiq/spell-checker",
    "type": "library",
    "description": "Code spell checker for PHP",
    "homepage": "http://github.com/paranoiq/spell-checker",
    "license": ["proprietary"],
    "authors": [
        {
            "name": "Vlasta Neubauer",
            "homepage": "http://www.paranoiq.cz"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "consistence/coding-standard": "2.0.0",
        "jakub-onderka/php-console-highlighter": "~0.3",
        "jakub-onderka/php-parallel-lint": "~0.9",
        "nette/tester": "~1.4",
        "phing/phing": "~2.16",
        "phpstan/phpstan": "~0.7",
        "slevomat/coding-standard": "3.0.1",
        "squizlabs/php_codesniffer": "3.0.1",
        "tracy/tracy": "~2.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:slevomat/consistence.git"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "classmap": ["src", "tests"]
    }
}
